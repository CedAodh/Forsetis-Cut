	_rlEnemyCheck ; 99/B1E6

		.al
		.xl
		.autsiz
		.databank ?

		lda #(`_EnemyTable)<<8
		sta lEventEngineLongParameter+1,b
		lda #<>_EnemyTable
		sta lEventEngineLongParameter,b
		jsl rlASMCCountAllUnitUncapturedAliveByTable
		lda wEventEngineParameter1,b
		bne +

		clc
		rtl

		+
		sec
		rtl

